/ {
    chosen {
        app-led = &led0;
    };
};

&i2c0 {
        status = "okay";
        pinctrl-0 = <&i2c0_default>;
	    pinctrl-names = "default";
        clock-frequency = <I2C_BITRATE_FAST>;
        si5351@60 {
                compatible = "silabs,si5351";
                reg = <0x60>;
        };
};

&pinctrl {
	i2c0_default: i2c0_default {
		group1 {
			pinmux = <I2C0_SDA_P4>, <I2C0_SCL_P5>;
			input-enable;
			input-schmitt-enable;
		};
	};
};
